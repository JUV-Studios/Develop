<UserControl
    x:Class="ProjectCodeEditor.Views.StartPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:ProjectCodeEditor.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="300"
    d:DesignWidth="400"
    mc:Ignorable="d">

    <Grid>
        <Grid.Resources>
            <ResourceDictionary Source="ms-appx:///Develop/HubStyles.xaml" />
        </Grid.Resources>
        <NavigationView
            CompactModeThresholdWidth="0"
            IsBackButtonVisible="Collapsed"
            IsPaneToggleButtonVisible="False"
            ItemInvoked="NavigationView_ItemInvoked"
            PaneDisplayMode="Auto">
            <Frame IsNavigationStackEnabled="False" SourcePageType="{x:Bind FramePageType, Mode=OneWay}" />
        </NavigationView>
    </Grid>
</UserControl>
