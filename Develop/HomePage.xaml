<UserControl
    x:Class="Develop.HomePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:Develop"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <NavigationView
        CompactModeThresholdWidth="{StaticResource MinWindowBreakpoint}"
        ExpandedModeThresholdWidth="{StaticResource MediumWindowBreakpoint}"
        IsBackButtonVisible="Collapsed"
        IsPaneToggleButtonVisible="False"
        ItemInvoked="NavigationView_ItemInvoked"
        PaneDisplayMode="Auto">
        <NavigationView.MenuItems />
        <Frame Background="{ThemeResource ModernPageBackground}" />
    </NavigationView>
</UserControl>
