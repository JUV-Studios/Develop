<Page
	x:Class="Develop.HomePage"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="using:Develop"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	NavigationCacheMode="Required"
	mc:Ignorable="d">

	<Page.Resources>
		<x:String x:Key="HomeTitleId">HomeNavigationItem/Content</x:String>
	</Page.Resources>
	<Hub Header="{Binding Source={StaticResource HomeTitleId}, Mode=OneTime, Converter={StaticResource ResourceController}}">
		<HubSection x:Uid="Home_GettingStarted" />
	</Hub>
</Page>
