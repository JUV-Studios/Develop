<Application
    x:Class="Develop.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Develop">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
            </ResourceDictionary.MergedDictionaries>
            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key="Default">
                    <StaticResource x:Key="ModernPageBackground" ResourceKey="SystemAltHighColor" />
                </ResourceDictionary>
                <ResourceDictionary x:Key="Dark">
                    <SolidColorBrush x:Key="ModernPageBackground" Color="#121212" />
                </ResourceDictionary>
            </ResourceDictionary.ThemeDictionaries>
            <x:Double x:Key="MinWindowBreakpoint">0</x:Double>
            <x:Double x:Key="MediumWindowBreakpoint">641</x:Double>
            <x:Double x:Key="LargeWindowBreakpoint">1008</x:Double>
            <ThemeResource x:Name="NavigationViewExpandedPaneBackground" ResourceKey="SystemControlAcrylicWindowBrush" />
            <ThemeResource x:Name="NavigationViewDefaultPaneBackground" ResourceKey="SystemControlAcrylicWindowBrush" />
        </ResourceDictionary>
    </Application.Resources>
</Application>